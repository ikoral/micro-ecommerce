{
  "name": "back-catalog",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "build": "npm-run-all 'build:dev -- --watch'",
    "build:dev": "cross-env NODE_ENV=development webpack --profile --progress --mode development",
    "build:pro": "cross-env NODE_ENV=production webpack -p --profile --progress --mode production",
    "clean": "rm -rf build",
    "lint": "cross-env NODE_ENV=development eslint . --fix",
    "start": "npm-run-all --parallel build start:dev",
    "start:dev": "cross-env NODE_ENV=development nodemon build/bundle.js",
    "start:debug": "cross-env NODE_ENV=development node --inspect build/bundle.js",
    "start:pro": "cross-env NODE_ENV=production node build/bundle.js"
  },
  "dependencies": {
    "apollo-server": "^2.9.15",
    "apollo-server-express": "^2.9.15",
    "common": "^0.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^4.0.2",
    "graphql": "^14.5.8",
    "mariadb": "^2.1.5",
    "sequelize": "^5.21.3",
    "webpack": "^4.41.5",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint-config-mmontes11-node": "^1.0.5",
    "eslint-import-resolver-webpack": "^0.12.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "webpack-cli": "^3.3.10"
  }
}
